<Window x:Class="AHDB.UI.Views.DeleteView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Delete" Height="150" Width="300" WindowStartupLocation="CenterScreen">
    <DockPanel LastChildFill="False">
        <Label Content="Are you sure you want to delete?" DockPanel.Dock="Top" HorizontalAlignment="Center"/>
        <Button Content="Delete" DockPanel.Dock="Left" Width="100" Height="25" Margin="5" Command="{Binding DeleteCommand}"/>
        <Button Content="Cancel" DockPanel.Dock="Right" Width="100" Height="25" Margin="5" Click="Button_Click"/>
    </DockPanel>
</Window>
